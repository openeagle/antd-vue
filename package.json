{
  "name": "@openeagle/antd-vue",
  "version": "1.0.0",
  "license": "MIT",
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "types": "./es/index.d.ts",
  "sideEffects": [
    "es/**/style.js",
    "es/**/style/*",
    "lib/**/style.js",
    "lib/**/style/*",
    "*.less"
  ],
  "files": [
    "babel-plugin-import.js",
    "es",
    "lib"
  ],
  "scripts": {
    "clean": "rimraf ./node_modules/.cache",
    "serve": "vue-cli-service serve ./examples/main.ts",
    "build": "npm run build:commonjs && npm run build:esmodule",
    "build:example": "cross-env ANALYZER=true vue-cli-service build ./examples/main.ts",
    "build:commonjs": "rimraf ./lib && babel -D --env-name commojs -x .ts,.tsx ./src -d ./lib && cross-env NODE_ENV=production tsc --project tsconfig.types.json --outDir lib",
    "build:esmodule": "rimraf ./es && babel -D --env-name esmodule -x .ts,.tsx ./src -d ./es && cross-env NODE_ENV=production tsc --project tsconfig.types.json --outDir es",
    "lint": "vue-cli-service lint",
    "precommit": "lint-staged",
    "storybook": "cross-env STORYBOOK=true start-storybook -p 6006",
    "build-storybook": "cross-env STORYBOOK=true build-storybook",
    "prerelease": "npm run build",
    "release": "semantic-release"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "peerDependencies": {
    "ant-design-vue": "^2.0.0"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.0.1",
    "@babel/runtime": "^7.1.1",
    "@tinymce/tinymce-vue": "^4.0.3",
    "@openeagle/eaxios": "^1.0.0",
    "accounting": "^0.4.1",
    "core-js": "^3.0.0",
    "lodash-es": "^4.17.21",
    "moment": "^2.29.1",
    "vue": "~3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@storybook/addon-actions": "^6.4.0-alpha.33",
    "@storybook/addon-console": "^1.2.3",
    "@storybook/addon-essentials": "^6.4.0-alpha.33",
    "@storybook/addon-links": "^6.4.0-alpha.33",
    "@storybook/addon-storysource": "^6.4.0-alpha.33",
    "@storybook/addons": "^6.4.0-alpha.33",
    "@storybook/theming": "^6.4.0-alpha.33",
    "@storybook/vue3": "^6.4.0-alpha.33",
    "@types/accounting": "^0.4.1",
    "@types/lodash-es": "^4.17.4",
    "@types/mockjs": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "@vue/cli-plugin-babel": "^4.5.0",
    "@vue/cli-plugin-eslint": "^4.5.0",
    "@vue/cli-plugin-typescript": "^4.5.0",
    "@vue/cli-service": "^4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@whitespace/storybook-addon-html": "^5.0.0",
    "ant-design-vue": "~2.1.6",
    "autodll-webpack-plugin": "^0.4.2",
    "babel-loader": "^8.2.2",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "babel-plugin-import": "^1.13.3",
    "cross-env": "^7.0.0",
    "dotenv-cli": "^4.0.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^7.0.0-0",
    "hard-source-webpack-plugin": "^0.13.1",
    "husky": "^5.2.0",
    "less": "^4.1.1",
    "less-loader": "^7.3.0",
    "lint-staged": "^10.5.4",
    "mockjs": "^1.1.0",
    "postcss": "^8.2.6",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.4.1",
    "typescript": "^4.2.2",
    "vue-loader": "^16.1.2",
    "vue-router": "^4.0.0",
    "vue-template-compiler": "^2.6.12",
    "webpack-bundle-analyzer": "^3.9.0"
  }
}
